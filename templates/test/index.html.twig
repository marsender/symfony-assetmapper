{% extends 'base.html.twig' %}

{% block body_id 'test' %}

{% block title %}
	{{ 'test.title'|trans }}
{% endblock %}

{% block body %}
	<div class="p-4 dark:text-white">
		<h1 class="text-2xl uppercase font-semibold">{{ 'test.title'|trans }}</h1>
		<div class="pt-4">
			<ul>
				{% for item in items %}
					{% set path = 'app_test_' ~ item %}
					<li>
						<a href="{{ path(path) }}">
							Test
							{{ item }}
						</a>
					</li>
				{% endfor %}
			</ul>
		</div>
	</div>
{% endblock %}
